@{
    ViewData["Title"] = "Home Page";

}
@if (TempData["DbVarification"] == null && TempData["NetVarification"] == null && TempData["trailVerion"] == null)
{
    <div class="page" ng-controller="companycontroller">
        <partial name="PartialViewHeader" />

        <div class="section-body mt-3">
            <div class="container-fluid">
                <div class="row clearfix">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="d-md-flex justify-content-between mb-2">
                                    <ul class="nav nav-tabs b-none">
                                        <li class="nav-item"><a class="nav-link active" data-toggle="tab" href="#list"><i class="fa fa-list-ul"></i> Companie List</a></li>
                                        <li class="nav-item"><a class="nav-link" data-toggle="tab" href="#addnew"><i class="fa fa-plus"></i> Add New</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section-body">
            <div class="container-fluid">
                <div class="tab-content">
                    <div class="tab-pane fade show active" id="list" role="tabpanel">
                        <div class="row clearfix">
                            <div class="col-xl-4 col-lg-4 col-md-6" ng-repeat="c in GetCompanyList">
                                <div class="card">
                                    <div class="card-body text-center ribbon">
                                        @* <div class="ribbon-box green">New</div> floorno *@
                                        <img class="rounded-circle img-thumbnail w100" src="~/assets/images/{{c.image}}" alt="">
                                        <h6 class="mt-3 mb-0">{{c.companyName}} </h6>
                                        <span>{{c.email}}</span> | <span>{{c.phone}}</span>
                                       @*  <ul class="mt-3 list-unstyled d-flex justify-content-center">
                                            <li><a class="p-3" target="_blank" href="#"><i class="fa fa-facebook"></i></a></li>
                                            <li><a class="p-3" target="_blank" href="#"><i class="fa fa-slack"></i></a></li>
                                            <li><a class="p-3" target="_blank" href="#"><i class="fa fa-linkedin"></i></a></li>
                                        </ul> *@
                                      @*   <button class="btn btn-default btn-sm">View Profile</button>
                                        <button class="btn btn-default btn-sm">Message</button> *@
                                        <div class="row text-center mt-4">
                                            <div class="col-6 border-right">
                                                <label class="mb-0">Guests</label>
                                                <h4 class="font-18">{{c.tottalguest}}</h4>
                                            </div>
                                            <div class="col-6">
                                                <label class="mb-0">Vistors</label>
                                                <h4 class="font-18">{{c.tottalvisitor}}</h4>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        </div>
                    </div>
                    <div class="tab-pane fade" id="addnew" role="tabpanel">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h3 class="card-title">Add Company</h3>
                                    </div>
                                    <form class="card-body">
                                        <div class="row clearfix">
                                            <div class="col-md-4 col-sm-12">
                                                <div class="form-group">
                                                    <label>Company Name</label>
                                                    <input type="text" id="companyName" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-2 col-sm-12">
                                                <label>Select Floor</label>
                                                <select class="form-control show-tick" id="company_floor">
                                                    <option value="">-- Floor --</option>
                                                    <option value="1">1st</option>
                                                    <option value="2">2nd</option>
                                                    <option value="3">3rd</option>
                                                    <option value="4">4th</option>
                                                    <option value="5">5th</option>
                                                    <option value="6">6th</option>
                                                    <option value="7">7th</option>
                                                    <option value="8">8th</option>
                                                    <option value="9">9th</option>
                                                    <option value="10">10th</option>
                                                    <option value="11">11th</option>
                                                    <option value="12">12th</option>
                                                    <option value="13">13th</option>
                                                    <option value="14">14th</option>
                                                    <option value="15">15th</option>
                                                </select>
                                            </div>
                                            <div class="col-md-3 col-sm-12">
                                                <div class="form-group">
                                                    <label>Company Email</label>
                                                    <input type="text" id="companyemail" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-3 col-sm-12">
                                                <div class="form-group">
                                                    <label>Company Phone</label>
                                                    <input type="text" id="companyphone" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-sm-12">
                                                <div class="form-group">
                                                    <input id="Logo" type="file" class="dropify">
                                                    <small class="text-danger">Recommended size is 3MB</small>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <button type="button" ng-click="SetCompanyFunction()" class="btn btn-primary">Save</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section-body">
            <footer class="footer">
                <div class="container-fluid">
                    @*  <div class="row">
                    <div class="col-md-6 col-sm-12">
                        <a href="templateshub.net">Templates Hub</a>
                    </div>
                    <div class="col-md-6 col-sm-12 text-md-right">
                        <ul class="list-inline mb-0">
                            <li class="list-inline-item"><a href="doc/index.html">Documentation</a></li>
                            <li class="list-inline-item"><a href="javascript:void(0)">FAQ</a></li>
                            <li class="list-inline-item"><a href="javascript:void(0)" class="btn btn-outline-primary btn-sm">Buy Now</a></li>
                        </ul>
                    </div>
                </div> *@
                </div>
            </footer>
        </div>
    </div>
}
else
{
    @Html.Partial("~/Views/Shared/PartialViewModalsValidation.cshtml")
}
<script src="~/assets/plugins/dropify/js/dropify.min.js"></script>
<script src="~/assets/js/form/dropify.js"></script>
<script src="~/assets/bundles/lib.vendor.bundle.js"></script>
<script src="~/lib/angularjs/angular.min.js"></script>
<script src="~/lib/angularjs/signalr.js"></script>
<script src="~/contollerscript/companycontroller.js"></script>
<script src="~/lib/angularjs/signalRService.js"></script>
<script src="~/assets/bundles/sweetalert.bundle.js"></script>